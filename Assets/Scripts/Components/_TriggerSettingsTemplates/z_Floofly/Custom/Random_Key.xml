<Init>
	<SetVariable Name="$Active" Value="1"/>
	<SetVariable Name="$AI" Value="1"/>
	<SetVariable Name="$Node" Value="COM"/>
	<SetVariable Name="Flag1" Value="0"/>
</Init>
<Loop>
	<Events>
		<Enter/>
	</Enter>
	<Actions>
		<Choose Order="Random" Set="1">
			<Press Key="Up" Model="_$Model"/>
			<Press Key="Down" Model="_$Model"/>
			<Press Key="Left" Model="_$Model"/>
			<Press Key="Right" Model="_$Model"/>
		</Choose>
	</Actions>
</Loop>